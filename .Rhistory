library(knitr)
library(gtrendsR)
library(tidyverse)
mydata<- read.csv("multiTimeline.csv")
kable(mydata)
# gtrends(
#   keyword = c("Kamala Harris", "Donald Trump", "Election"),
#   geo = "US",
#   time = "today 12-m",
#   compared_breakdown = TRUE
# )
mydata <- gtrends(
keyword = c("Kamala Harris", "Donald Trump", "Election"),
geo = "US",
time = "today 12-m"
)
interest_data <- mydata$interest_over_time
monthly_trends <- interest_data %>%
mutate(month_year = floor_date(date, "month")) %>%
group_by(keyword, month_year) %>%
summarise(average_hits = mean(hits, na.rm = TRUE)) %>%
ungroup()
ggplot(monthly_trends, aes(x = month_year, y = average_hits, color = keyword)) +
geom_line(size = 1.2) +
labs(
title = "Monthly Google Trends for 2024 Election Keywords",
subtitle = "Average search interest over time",
x = "Date",
y = "Relative Search Interest (0-100)",
color = "Keyword"
) +
theme_minimal() +
scale_x_date(date_breaks = "1 month", date_labels = "%b %Y") +
theme(axis.text.x = element_text(angle = 45, hjust = 1))
